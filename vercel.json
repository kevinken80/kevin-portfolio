{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.ts",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "installCommand": "npm install && npm install -D @vitejs/plugin-react",
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
}
